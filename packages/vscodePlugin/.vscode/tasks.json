{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "yarn:watch",
			"type": "npm",
			"script": "watch",
			"problemMatcher": [
				"$tsc-watch",
				"$eslint-compact"
			],
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}